<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.Plugin.CameraView</id>
    <version>4.1.216</version>
    <title>Camera View for Zebble for Xamarin</title>
    <description>Displays a live feed from the camera.</description>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.CameraView/master/Shared/Icon.png</iconUrl>
    <authors>Mobile App Technologies Ltd</authors>
    <owners>Mobile App Technologies Ltd</owners>
    <copyright>Copyright Â©2017 | Mobile App Technologies Ltd</copyright>
    <projectUrl>https://github.com/Geeksltd/Zebble.CameraView</projectUrl>

    <dependencies>
      <group targetFramework="UAP10">
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
      <group targetFramework="MonoAndroid">
        <dependency id="Zebble" version= "4.0.239"/>
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Zebble" version= "4.0.239"/>
        <dependency id="HtmlAgilityPack" version="1.4.9.5" />
      </group>
    </dependencies>

  </metadata>
  <files>
    <file src="..\..\lib\Zebble.CameraView.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.CameraView.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.CameraView.UWP.*" target="lib\UAP10\" />
  </files>
</package>